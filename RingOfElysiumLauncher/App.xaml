<Application x:Class="RingOfElysiumLauncher.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:RingOfElysiumLauncher"
             Startup="Application_Startup">

    <Application.Resources>
        
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="Styles/WindowStyle.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/SettingsButton.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/SettingsCloseButton.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/PlayButton.xaml"></ResourceDictionary>
                <ResourceDictionary Source="Styles/SettingsStyle.xaml"></ResourceDictionary>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
        
    </Application.Resources>
</Application>
